{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "env": {
    "browser": true,
    "node": true
  },
  "rules": {
    "comma-dangle": 0,
    "indent": [2, 2],
    "new-cap": "off",
    "no-else-return": 0,
    "no-mixed-requires": [0],
    "no-underscore-dangle": [0],
    "no-unreachable": "error",
    "quotes": [2, "single"],
    "semi": "error"
  }
}
